<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00ffffff"
    android:orientation="vertical" >
    <FrameLayout android:layout_height="wrap_content"
            android:layout_width="wrap_content">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:orientation="vertical" 
            android:background="@drawable/f_bg"><!-- f_bg  umeng_fb_list_item_pressed-->

            <TextView
                android:id="@+id/f_memory_total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>

            <TextView
                android:id="@+id/f_memory_avail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>

            <TextView
                android:id="@+id/f_memory_low"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>

            <TextView
                android:id="@+id/f_memory_memSize"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>

            <TextView
                android:id="@+id/f_memory_pid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>

            <TextView
                android:id="@+id/f_memory_cpuUsage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff"
                android:text="当前进程所占cpu：0.0%" android:textSize="10dp"/>

            <TextView
                android:id="@+id/f_memory_processName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>
            
			<TextView
                android:id="@+id/f_memory_topactivity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>
			
			<TextView
                android:id="@+id/f_memory_topactivity_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#ffffff" android:textSize="10dp"/>
           
        </LinearLayout>
        <ImageView
                android:id="@+id/f_logo"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:paddingLeft="4dp"
                android:paddingTop="4dp"
                android:src="@drawable/close_a"
                android:layout_gravity="right"/>
    </FrameLayout>

</LinearLayout>